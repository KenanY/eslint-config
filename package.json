{
  "name": "@kenan/eslint-config",
  "version": "6.0.0",
  "description": "KenanY's eslint config",
  "keywords": [
    "eslint-config"
  ],
  "repository": "KenanY/eslint-config",
  "license": "MIT",
  "author": "Kenan Yildirim <kenan@kenany.me> (http://kenany.me/)",
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE.txt"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "find-unused": "eslint-find-rules --unused",
    "pretest": "npm run -s lint",
    "test": "tape test/*.js",
    "prepublish": "(in-install || npm run find-unused) && (not-in-publish || npm test)",
    "lint": "eslint *.js test/*.js"
  },
  "dependencies": {
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "in-publish": "^2.0.0"
  },
  "devDependencies": {
    "@kenan/renovate-config": "1.1.1",
    "cross-spawn": "6.0.5",
    "eslint": "5.16.0",
    "eslint-find-rules": "3.4.0",
    "graceful-fs": "4.2.2",
    "lodash.isplainobject": "4.0.6",
    "mkdirp": "0.5.1",
    "run-parallel-limit": "1.0.5",
    "standard-engine": "10.0.0",
    "tape": "4.11.0"
  },
  "peerDependencies": {
    "eslint": "^5.0.0"
  }
}
