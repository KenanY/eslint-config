{
  "name": "@kenan/eslint-config",
  "version": "4.0.0",
  "description": "KenanY's eslint config",
  "keywords": [
    "eslint-config"
  ],
  "repository": "KenanY/eslint-config",
  "license": "MIT",
  "author": "Kenan Yildirim <kenan@kenany.me> (http://kenany.me/)",
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE.txt"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "npm run -s lint",
    "test": "tape test/*.js",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || npm test)",
    "lint": "eslint *.js test/*.js"
  },
  "dependencies": {
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "in-publish": "^2.0.0"
  },
  "devDependencies": {
    "cross-spawn": "^5.0.0",
    "eslint": "^3.4.0",
    "eslint-find-rules": "^1.13.0",
    "graceful-fs": "^4.1.6",
    "lodash.isplainobject": "^4.0.4",
    "mkdirp": "^0.5.1",
    "run-parallel-limit": "^1.0.3",
    "standard-engine": "^5.1.0",
    "tape": "^4.6.0"
  },
  "peerDependencies": {
    "eslint": "^3.4.0"
  }
}
