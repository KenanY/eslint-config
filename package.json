{
  "name": "@kenan/eslint-config",
  "version": "3.0.0",
  "description": "KenanY's eslint config",
  "keywords": [
    "eslint-config"
  ],
  "repository": "KenanY/eslint-config",
  "license": "MIT",
  "author": "Kenan Yildirim <kenan@kenany.me> (http://kenany.me/)",
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE.txt"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "npm run -s lint",
    "test": "tape test/index.js",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || npm test)",
    "lint": "eslint *.js test/index.js"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-find-rules": "^1.13.0",
    "in-publish": "^2.0.0",
    "lodash.isplainobject": "^4.0.4",
    "tape": "^4.5.1"
  },
  "peerDependencies": {
    "eslint": "^3.4.0"
  }
}
